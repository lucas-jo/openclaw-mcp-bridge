{
  "name": "openclaw-bridge-remote",
  "version": "0.2.1",
  "description": "Model Context Protocol (MCP) bridge for OpenClaw Gateway",
  "main": "src/index.ts",
  "bin": {
    "openclaw-bridge-remote": "./src/index.ts"
  },
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "openclaw",
    "bridge",
    "automation",
    "browser-control",
    "remote-agent"
  ],
  "author": "Lucas Jo",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "dotenv": "^17.2.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "bun-types": "latest",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
